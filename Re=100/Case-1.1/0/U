FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.00604 0 0);

boundaryField
{
    FrtBck
    {
        type            empty;
    }
    Top
    {
        type            symmetryPlane;
    }
    Inlet
    {
        type            fixedValue;
        value           uniform (0.00604 0 0);
    }
    Btm
    {
        type            symmetryPlane;
    }
    Outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0.00604 0 0);
    }
    Cylinder
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


